\">\n                        <div class=\"row\">\n                            <div class=\"col-sm-6\">\n                                <h2 class=\"titre-admin\">Manage <b>Articles</b></h2>\n                            </div>\n                            <div class=\"col-sm-6 mt-2\">\n                                <button id=\"show-modal\" class=\"btn btn-success mr-2\" @click=\"showModal = true\">+ Add New Article</button>\n                            </div>\n                        </div>\n                    </div>\n                    <table class=\"table table-striped table-hover\">\n                        <thead>\n                            <tr>\n                                <th>Title</th>\n                                <th>Description</th>\n                                <th>Delete</th>\n                                <th>Update</th>\n                                <th>Show</th>\n                            </tr>\n                        </thead>\n                        <tbody>\n\n                            <tr v-for=\"article in getAllArticles.data\" :key=\"article.id\">\n\n\n\n                                <td>{{article.title}}</td>\n                                <td>{{article.description}}</td>\n                                <td>\n                                        <button class=\"delete btn btn-danger\" @click.prevent=\"ondelete(article.id)\"\n                                            name=\"delete_data_btn\">\n                                            <svg width=\"1em\" height=\"1em\" viewBox=\"0 0 16 16\"\n                                                class=\"bi bi-trash-fill\" fill=\"currentColor\"\n                                                xmlns=\"http://www.w3.org/2000/svg\">\n                                                <path fill-rule=\"evenodd\"\n                                                    d=\"M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10